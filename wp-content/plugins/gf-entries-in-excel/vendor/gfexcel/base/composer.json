{
  "name": "gfexcel/base",
  "description": "Base for GravityExport",
  "type": "library",
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "php-mock/php-mock-phpunit": "^2.5",
    "overtrue/phplint": "^2.3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Doeke Norg",
      "email": "info@gfexcel.com"
    }
  ],
  "require": {
    "league/container": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "GFExcel\\" : "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GFExcel\\Tests\\": "./tests"
    }
  },
  "scripts": {
    "linter": "vendor/bin/phplint --no-cache",
    "test": "vendor/bin/phpunit"
  }
}
